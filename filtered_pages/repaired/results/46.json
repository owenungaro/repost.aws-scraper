{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "UserID",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": "arn:aws:s3:::mybucket/*",
      "Condition": {
        "StringLike": {
          "aws:userId": "AROAJI4AVV3EXAMPLEID:SSOUserName"
        }
      }
    }
  ]
}