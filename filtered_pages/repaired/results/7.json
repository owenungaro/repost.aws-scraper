{
  "Sid": "DenyAllExceptSpecifiedVPC",
  "Effect": "Deny",
  "Principal": "*",
  "Action": "s3:*",
  "Resource": [
    "arn:aws:s3:::s3-document",
    "arn:aws:s3:::s3-document/*"
  ],
  "Condition": {
    "StringNotLike": {
      "aws:userid": "AROASAMPLESAMPLESAMPL:*"
    },
    "StringNotEquals": {
      "aws:sourceVpc": "<my VPC ID>"
    }
  }
}