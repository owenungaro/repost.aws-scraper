{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "SSMLogging",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::SSM_account_id:root"
      },
      "Action": [
        "s3:PutObjectAcl",
        "s3:PutObject",
        "s3:GetEncryptionConfiguration"
      ],
      "Resource": [
        "arn:aws:s3:::bucket_name/*",
        "arn:aws:s3:::bucket_name"
      ]
    }
  ]
}