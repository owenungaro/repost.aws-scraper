{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": "execute-api:Invoke",
      "Resource": "arn:aws:execute-api:us-east-1:5XXXXXXXXXX7:lxxxxxxxxxk/staging/GET/myapi1/*"
    },
    {
      "Effect": "Deny",
      "Principal": "*",
      "Action": "execute-api:Invoke",
      "Resource": "arn:aws:execute-api:us-east-1:5XXXXXXXXXX7:lxxxxxxxxxk/staging/GET/myapi1/*",
      "Condition": {
        "StringNotLike": {
          "aws:RequestArn": [
            "arn:aws:execute-api:us-east-1:5XXXXXXXXXX7:lxxxxxxxxxk/staging/GET/myapi1/status",
            "arn:aws:execute-api:us-east-1:5XXXXXXXXXX7:lxxxxxxxxxk/staging/GET/myapi1/v1/*"
          ]
        }
      }
    }
  ]
}