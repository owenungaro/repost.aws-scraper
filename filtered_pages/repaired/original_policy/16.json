{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Deny",
      "Principal": {
        "AWS": "*"
      },
      "Action": [
        "s3:GetObject",
        "s3:ListBucket",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::admin-only-bucket",
        "arn:aws:s3:::admin-only-bucket/*"
      ],
      "Condition": {
        "StringNotEquals": {
          "s3:DataAccessPointAccount": "xxxxxxxxxxxx"
        }
      }
    }
  ]
}