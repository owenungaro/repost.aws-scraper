{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "iam:GetRole",
        "iam:ListRoleTags",
        "iam:ListRoles",
        "iam:ListRolePolicies",
        "iam:GetRolePolicy",
        "iam:PassRole",
        "iam:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "ListEc2AndListInstanceProfiles",
      "Effect": "Allow",
      "Action": [
        "iam:ListInstanceProfiles",
        "ec2:Describe*",
        "ec2:Search*",
        "ec2:Get*"
      ],
      "Resource": "*"
    }
  ]
}