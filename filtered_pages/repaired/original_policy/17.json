{
  "Sid": "AWSSSMWrite",
  "Effect": "Allow",
  "Principal": {
    "AWS": "arn:aws-us-gov:iam::<acct#>:root"
  },
  "Action": "s3:PutObject",
  "Resource": "arn:aws-us-gov:s3:::prod-ssm/Patching/*",
  "Condition": {
    "StringEquals": {
      "s3:x-amz-acl": "bucket-owner-full-control"
    }
  }
}