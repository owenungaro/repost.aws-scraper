{
  "Statement": [
    {
      "Action": [
        "elasticache:DescribeSnapshots",
        "elasticache:CopySnapshot"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:GetBucketAcl",
        "s3:ListMultipartUploadParts",
        "s3:ListBucketMultipartUploads",
        "s3:GetBucketLocation",
        "s3:ListAllMyBuckets"
      ],
      "Effect": "Allow",
      "Resource": [
        "BUCKET_NAME/*",
        "BUCKET_NAME"
      ]
    }
  ],
  "Version": "2012-10-17"
}