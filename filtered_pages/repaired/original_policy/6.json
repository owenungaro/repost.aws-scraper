{
  "Version": "2012-10-17",
  "Id": "pl-sm_ev_vpce_ecs_sr",
  "Statement": [
    {
      "Sid": "RestrictGetSecretValueoperation",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "secretsmanager:GetSecretValue",
      "Resource": "*",
      "Condition": {
        "StringNotEquals": {
          "aws:sourceVpce": "vpce-myvpce"
        }
      }
    }
  ]
}