{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": "execute-api:Invoke",
      "Resource": "arn:aws:execute-api:eu-central-1:012345678901:someid/prod/POST/resource",
      "Condition": {
        "StringEquals": {
          "aws:ResourceOrgPaths": "o-orgid/r-rootid/ou-firstlevel/ou-secondlevel"
        }
      }
    }
  ]
}