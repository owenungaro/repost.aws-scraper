{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Deny",
      "Action": [
        "dataexchange:AcceptDataGrant"
      ],
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "aws:PrincipalOrgID": "ORGID"
        },
        "StringNotEquals": {
          "aws:PrincipalAccount": "ACCOUNTID"
        }
      }
    }
  ]
}