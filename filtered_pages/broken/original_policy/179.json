{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyAllTagProd",
      "Effect": "Deny",
      "Action": [
        "*"
      ],
      "Resource": [
        "*"
      ],
      "Condition": {
        "StringEquals": {
          "aws:ResourceTag/env": "prod"
        }
      }
    }
  ]
}