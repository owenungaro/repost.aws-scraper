{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Statement1",
      "Effect": "Allow",
      "Action": [
        "iam:PassRole",
        "backup:StartCopyJob",
        "backup:DescribeRecoveryPoint",
        "backup:ListTags"
      ],
      "Resource": [
        "arn:aws:backup:us-east-2:222222222222:backup-vault:zzush-intermediate-vault-ohio"
      ]
    },
    {
      "Sid": "Statement2",
      "Effect": "Allow",
      "Action": [
        "kms:"
      ],
      "Resource": ""
    },
    {
      "Sid": "Statement3",
      "Effect": "Allow",
      "Action": [
        "sts:AssumeRole"
      ],
      "Resource": [
        "arn:aws:iam::222222222222:role/",
        "arn:aws:iam::777777777777:role/"
      ]
    },
    {
      "Sid": "Statement4",
      "Effect": "Allow",
      "Action": [
        "logs:PutLogEvents",
        "logs:CreateLogStream",
        "logs:CreateLogGroup"
      ],
      "Resource": [
        "arn:aws:logs:::*"
      ]
    }
  ]
}