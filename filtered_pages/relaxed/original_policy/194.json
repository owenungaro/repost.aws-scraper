{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "kms:ReEncrypt*",
        "kms:GetKeyPolicy",
        "kms:GenerateDataKey*",
        "kms:Encrypt",
        "kms:DescribeKey",
        "kms:Decrypt",
        "kms:CreateGrant"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:kms:us-east-1:<account id>:key/<key for bucketA>",
      "Sid": "KMSUsage"
    }
  ]
}