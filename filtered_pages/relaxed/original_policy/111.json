{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "Logging",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:log-group:/aws/lambda/MyLambda_*"
    },
    {
      "Sid": "S3",
      "Effect": "Allow",
      "Action": "s3:PutObject",
      "Resource": [
        "arn:aws:s3:::my-bucket-test/*"
      ]
    },
    {
      "Sid": "S3Prod",
      "Effect": "Allow",
      "Action": "s3:PutObject",
      "Resource": "arn:aws:s3:::my-bucket-data/*",
      "Condition": {
        "StringLike": {
          "lambda:SourceFunctionArn": "*Prod"
        }
      }
    }
  ]
}