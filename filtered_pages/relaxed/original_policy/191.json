{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AlexTestPolicy",
      "Effect": "Allow",
      "Action": [
        "ec2:StartInstances"
      ],
      "Resource": "*",
      "Condition": {
        "StringLike": {
          "aws:PrincipalTag/Division": "*${ec2:ResourceTag/Division}*"
        }
      }
    }
  ]
}