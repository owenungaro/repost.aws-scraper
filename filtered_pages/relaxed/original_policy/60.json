{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Deny",
      "Principal": "*",
      "Action": "execute-api:Invoke",
      "Resource": [
        "arn:aws:execute-api:ap-northeast-2:0xxxxxxxxxx2:rxxxxxx2/*/GET/user/*"
      ]
    },
    {
      "Effect": "Allow",
      "Principal": "*",
      "Action": "execute-api:Invoke",
      "Resource": [
        "arn:aws:execute-api:ap-northeast-2:0xxxxxxxxxx2:rxxxxxx2/*/GET/user/*/api/*"
      ]
    }
  ]
}