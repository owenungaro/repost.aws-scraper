{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowVpcEndpoint",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::great-expectations-docs",
        "arn:aws:s3:::great-expectations-docs/*"
      ],
      "Condition": {
        "StringEquals": {
          "aws:sourceVpce": "vpce-0c0f4bb767f820bda"
        }
      }
    }
  ]
}