{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::123:role/roletest1"
      },
      "Action": [
        "s3tables:GetTableData",
        "s3tables:GetTableMetadataLocation"
      ],
      "Resource": "arn:aws:s3tables:us-west-2:123:table-bucket/tabletest/table/*",
      "Condition": {
        "StringLike": {
          "s3tables:TableNamespace": "namesptest"
        }
      }
    }
  ]
}